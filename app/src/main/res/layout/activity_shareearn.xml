<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">


    <include
        android:id="@+id/mToolbar"
        layout="@layout/bill_payment_layout" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="@dimen/_45sdp"
        app:layout_constraintTop_toBottomOf="@id/mToolbar">

<!--        <androidx.swiperefreshlayout.widget.SwipeRefreshLayout-->
<!--            android:id="@+id/swipeRefreshLayout"-->
<!--            android:layout_width="match_parent"-->
<!--            android:layout_height="match_parent"-->
<!--            >-->


        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@color/white"
                android:orientation="vertical">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/mToolbar_constraint"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_160sdp"
                    android:background="@drawable/help_section_shape"
                    app:layout_constraintTop_toTopOf="parent">


                    <androidx.viewpager2.widget.ViewPager2
                        android:id="@+id/viewPager"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent" />

                    <com.tbuonomo.viewpagerdotsindicator.DotsIndicator
                        android:id="@+id/worm_dots_indicator"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        app:dotsColor="@color/gray"
                        app:dotsCornerRadius="@dimen/_2sdp"
                        app:selectedDotColor="@color/colorPrimaryB"
                        app:dotsSize="@dimen/_3sdp"
                        app:dotsSpacing="@dimen/_2sdp"
                        android:layout_marginBottom="@dimen/_10sdp"
                        android:visibility="visible"
                        app:layout_constraintStart_toStartOf="@id/viewPager"
                        app:layout_constraintEnd_toEndOf="@id/viewPager"
                        app:layout_constraintBottom_toBottomOf="@id/viewPager"/>


                </androidx.constraintlayout.widget.ConstraintLayout>

                <View
                    android:id="@+id/myBonusView"
                    android:layout_width="@dimen/_125sdp"
                    android:layout_height="@dimen/_78sdp"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:background="@drawable/my_bonus_shape"
                    app:layout_constraintEnd_toStartOf="@id/maxPointsView"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/mToolbar_constraint" />

                <ImageView
                    android:id="@+id/myBonusImg"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/my_bonus"
                    app:layout_constraintBottom_toTopOf="@id/myBonusTV"
                    app:layout_constraintEnd_toEndOf="@id/myBonusView"
                    app:layout_constraintStart_toStartOf="@id/myBonusView"
                    app:layout_constraintTop_toTopOf="@id/myBonusView" />

                <TextView
                    android:id="@+id/myBonusTV"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:text="@string/my_bonus"
                    android:textColor="#0B54A5"
                    android:textSize="@dimen/_11sdp"
                    android:textStyle="bold"
                    app:layout_constraintBottom_toTopOf="@id/myBonus_availableTxt"
                    app:layout_constraintEnd_toEndOf="@id/myBonusView"
                    app:layout_constraintStart_toStartOf="@id/myBonusView"
                    app:layout_constraintTop_toBottomOf="@id/myBonusImg" />

                <TextView
                    android:id="@+id/myBonus_availableTxt"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text=""
                    android:textColor="#0B54A5"
                    android:textSize="@dimen/_17sdp"
                    android:textStyle="bold"
                    app:layout_constraintBottom_toBottomOf="@id/myBonusView"
                    app:layout_constraintEnd_toEndOf="@id/myBonusView"
                    app:layout_constraintStart_toStartOf="@id/myBonusView"
                    app:layout_constraintTop_toBottomOf="@id/myBonusTV" />

                <View
                    android:id="@+id/maxPointsView"
                    android:layout_width="@dimen/_125sdp"
                    android:layout_height="@dimen/_78sdp"
                    android:background="@drawable/max_points_share_shape"
                    app:layout_constraintBottom_toBottomOf="@id/myBonusView"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toEndOf="@id/myBonusView"
                    app:layout_constraintTop_toTopOf="@id/myBonusView" />

                <ImageView
                    android:id="@+id/maxPointsImg"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/max_points_orange_icon"
                    app:layout_constraintBottom_toTopOf="@id/maxPointsTV"
                    app:layout_constraintEnd_toEndOf="@id/maxPointsView"
                    app:layout_constraintStart_toStartOf="@id/maxPointsView"
                    app:layout_constraintTop_toTopOf="@id/maxPointsView" />

                <TextView
                    android:id="@+id/maxPointsTV"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:text="@string/max_points"
                    android:textColor="#FF9800"
                    android:textSize="@dimen/_11sdp"
                    android:textStyle="bold"
                    app:layout_constraintBottom_toTopOf="@id/maxPoints_availableTxt"
                    app:layout_constraintEnd_toEndOf="@id/maxPointsView"
                    app:layout_constraintStart_toStartOf="@id/maxPointsView"
                    app:layout_constraintTop_toBottomOf="@id/maxPointsImg" />

                <TextView
                    android:id="@+id/maxPoints_availableTxt"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text=""
                    android:textColor="#FF9800"
                    android:textSize="@dimen/_17sdp"
                    android:textStyle="bold"
                    app:layout_constraintBottom_toBottomOf="@id/maxPointsView"
                    app:layout_constraintEnd_toEndOf="@id/maxPointsView"
                    app:layout_constraintStart_toStartOf="@id/maxPointsView"
                    app:layout_constraintTop_toBottomOf="@id/maxPointsTV" />


                <View
                    android:id="@+id/SuccessfulReferralView"
                    android:layout_width="@dimen/_80sdp"
                    android:layout_height="@dimen/_80sdp"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:background="@drawable/successfull_referral_shape"
                    app:layout_constraintEnd_toStartOf="@id/earnView"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/myBonusView" />

                <ImageView
                    android:id="@+id/SuccessfulReferralImg"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/successfull_referral_icon"
                    app:layout_constraintBottom_toTopOf="@id/available_SuccessfulReferralTV"
                    app:layout_constraintEnd_toEndOf="@id/SuccessfulReferralView"
                    app:layout_constraintStart_toStartOf="@id/SuccessfulReferralView"
                    app:layout_constraintTop_toTopOf="@id/SuccessfulReferralView" />

                <TextView
                    android:id="@+id/available_SuccessfulReferralTV"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:text=""
                    android:textColor="#6766BD"
                    android:textSize="@dimen/_14sdp"
                    android:textStyle="bold"
                    app:layout_constraintBottom_toTopOf="@id/SuccessfulReferralTxt"
                    app:layout_constraintEnd_toEndOf="@id/SuccessfulReferralView"
                    app:layout_constraintStart_toStartOf="@id/SuccessfulReferralView"
                    app:layout_constraintTop_toBottomOf="@id/SuccessfulReferralImg" />

                <TextView
                    android:id="@+id/SuccessfulReferralTxt"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/successful_n_referral"
                    android:textColor="#6766BD"
                    android:textSize="@dimen/_8sdp"
                    android:textStyle="bold"
                    app:layout_constraintBottom_toBottomOf="@id/SuccessfulReferralView"
                    app:layout_constraintEnd_toEndOf="@id/SuccessfulReferralView"
                    app:layout_constraintStart_toStartOf="@id/SuccessfulReferralView"
                    app:layout_constraintTop_toBottomOf="@id/available_SuccessfulReferralTV" />

                <View
                    android:id="@+id/earnView"
                    android:layout_width="@dimen/_80sdp"
                    android:layout_height="@dimen/_80sdp"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:background="@drawable/remaining_referral_icon"
                    app:layout_constraintEnd_toStartOf="@id/referralView"
                    app:layout_constraintStart_toEndOf="@id/SuccessfulReferralView"
                    app:layout_constraintTop_toBottomOf="@id/myBonusView"

                    />

                <ImageView
                    android:id="@+id/earnImg"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/referral_svg"
                    app:layout_constraintBottom_toTopOf="@id/available_earnTV"
                    app:layout_constraintEnd_toEndOf="@id/earnView"
                    app:layout_constraintStart_toStartOf="@id/earnView"
                    app:layout_constraintTop_toTopOf="@id/earnView" />

                <TextView
                    android:id="@+id/available_earnTV"
                    android:layout_width="@dimen/_70sdp"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:text="@string/impty"
                    android:textColor="#064E3B"
                    android:maxLines="1"
                    android:textSize="@dimen/_14sdp"
                    android:textStyle="bold"
                    app:layout_constraintBottom_toTopOf="@id/earnTxt"
                    app:layout_constraintEnd_toEndOf="@id/earnView"
                    app:layout_constraintStart_toStartOf="@id/earnView"
                    app:layout_constraintTop_toBottomOf="@id/earnImg" />

                <TextView
                    android:id="@+id/earnTxt"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:text="@string/remaining_nreferral"
                    android:textColor="#064E3B"
                    android:textSize="@dimen/_8sdp"
                    android:textStyle="bold"
                    app:layout_constraintBottom_toBottomOf="@id/earnView"
                    app:layout_constraintEnd_toEndOf="@id/earnView"
                    app:layout_constraintStart_toStartOf="@id/earnView"
                    app:layout_constraintTop_toBottomOf="@id/available_earnTV" />

                <View
                    android:id="@+id/referralView"
                    android:layout_width="@dimen/_80sdp"
                    android:layout_height="@dimen/_80sdp"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:background="@drawable/total_referral_shape"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toEndOf="@id/earnView"
                    app:layout_constraintTop_toBottomOf="@id/myBonusView" />

                <ImageView
                    android:id="@+id/referralImg"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/total_referral_icon"
                    app:layout_constraintBottom_toTopOf="@id/available_referralTV"
                    app:layout_constraintEnd_toEndOf="@id/referralView"
                    app:layout_constraintStart_toStartOf="@id/referralView"
                    app:layout_constraintTop_toTopOf="@id/referralView" />

                <TextView
                    android:id="@+id/available_referralTV"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:text=""
                    android:textColor="#0B53A4"
                    android:textSize="@dimen/_14sdp"
                    android:textStyle="bold"
                    app:layout_constraintBottom_toTopOf="@id/referralTxt"
                    app:layout_constraintEnd_toEndOf="@id/referralView"
                    app:layout_constraintStart_toStartOf="@id/referralView"
                    app:layout_constraintTop_toBottomOf="@id/referralImg" />

                <TextView
                    android:id="@+id/referralTxt"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/total_referral"
                    android:textColor="#0B53A4"
                    android:textSize="@dimen/_8sdp"
                    android:textStyle="bold"
                    app:layout_constraintBottom_toBottomOf="@id/referralView"
                    app:layout_constraintEnd_toEndOf="@id/referralView"
                    app:layout_constraintStart_toStartOf="@id/referralView"
                    app:layout_constraintTop_toBottomOf="@id/available_referralTV" />


                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/referral_link_ConstraintLayout"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_20sdp"
                    app:layout_constraintTop_toBottomOf="@id/SuccessfulReferralView">

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/referral_link"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/_45sdp"
                        android:layout_marginHorizontal="@dimen/_15sdp"
                        android:layout_marginTop="@dimen/_5sdp"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent">

                        <com.google.android.material.textfield.TextInputLayout
                            android:id="@+id/inputField"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            app:boxBackgroundColor="@color/white"
                            app:boxBackgroundMode="outline"
                            app:boxCornerRadiusBottomEnd="@dimen/_9sdp"
                            app:boxCornerRadiusBottomStart="@dimen/_9sdp"
                            app:boxCornerRadiusTopEnd="@dimen/_9sdp"
                            app:boxCornerRadiusTopStart="@dimen/_9sdp"
                            app:boxStrokeColor="#626262"
                            app:hintTextColor="#626262"
                            app:boxStrokeWidth="@dimen/_1sdp"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent">

                            <EditText
                                android:id="@+id/txtReferId"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:cursorVisible="false"
                                android:enabled="false"
                                android:focusable="false"
                                android:gravity="center"
                                android:hint="@string/your_referral_code"
                                android:paddingTop="@dimen/_5sdp"
                                android:textColor="@color/black"
                                android:textColorHint="@color/black"
                                android:textSize="@dimen/_16sdp"
                                android:textStyle="bold" />


                        </com.google.android.material.textfield.TextInputLayout>

                        <TextView
                            android:id="@+id/btnReferCopy"
                            android:layout_width="@dimen/_45sdp"
                            android:layout_height="@dimen/_33sdp"
                            android:layout_marginTop="@dimen/_3sdp"
                            android:layout_marginEnd="@dimen/_3sdp"
                            android:background="@drawable/ad_money_button_shape"
                            android:gravity="center"
                            android:text="@string/copy"
                            android:textColor="@color/white"
                            android:textSize="@dimen/_9sdp"
                            android:textStyle="bold"
                            app:layout_constraintBottom_toBottomOf="@+id/inputField"
                            app:layout_constraintEnd_toEndOf="@+id/inputField"
                            app:layout_constraintTop_toTopOf="@+id/inputField" />
                    </androidx.constraintlayout.widget.ConstraintLayout>

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/btnInvite"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/_38sdp"
                        android:layout_marginHorizontal="@dimen/_15sdp"
                        android:layout_marginTop="@dimen/_20sdp"
                        android:background="@drawable/share_app_button_shape"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@id/referral_link">

                        <androidx.constraintlayout.widget.ConstraintLayout
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent">

                            <ImageView
                                android:id="@+id/share_img"
                                android:layout_width="@dimen/_15sdp"
                                android:layout_height="@dimen/_15sdp"
                                android:layout_marginStart="@dimen/_10sdp"
                                android:src="@drawable/share_img_svg"
                                app:layout_constraintBottom_toBottomOf="@id/shareTextView"
                                app:layout_constraintStart_toEndOf="@id/shareTextView"
                                app:layout_constraintTop_toTopOf="@id/shareTextView" />

                            <TextView
                                android:id="@+id/shareTextView"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/share"
                                android:textColor="@color/white"
                                android:textSize="@dimen/_17sdp"
                                android:textStyle="normal"
                                app:layout_constraintBottom_toBottomOf="parent"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintTop_toTopOf="parent" />

                        </androidx.constraintlayout.widget.ConstraintLayout>

                    </androidx.constraintlayout.widget.ConstraintLayout>


                </androidx.constraintlayout.widget.ConstraintLayout>

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/c"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginHorizontal="@dimen/_15sdp"
                    android:layout_marginTop="@dimen/_15sdp"
                    android:elevation="@dimen/_3sdp"
                    android:paddingBottom="@dimen/_12sdp"
                    android:paddingTop="@dimen/_12sdp"
                    app:layout_constraintTop_toBottomOf="@+id/referral_link_ConstraintLayout">


                    <WebView
                        android:id="@+id/webView"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        tools:ignore="WebViewLayout"
                        />

